{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"leaflet/dist/leaflet.css\";\nimport { LMap, LTileLayer, LControl, LMarker, LPopup } from \"@vue-leaflet/vue-leaflet\";\nexport default {\n  components: {\n    LMap,\n    LTileLayer,\n    LControl,\n    LPopup,\n    LMarker\n  },\n  data() {\n    return {\n      zoom: 17,\n      center: [47.70571705924626, -122.1419059757434],\n      markerCoords: [47.70446575151325, -122.1422571803662],\n      annotations: []\n    };\n  },\n  methods: {\n    resetViewHandler() {\n      this.center = [47.70571705924626, -122.1419059757434];\n      this.zoom = 17;\n    },\n    addAnnotationLeaflet(event) {\n      // Ensure that event has latlng property\n      if (event && event.latlng) {\n        // Extract latitude and longitude from the click event\n        const lat = event.latlng.lat;\n        const lng = event.latlng.lng;\n\n        // Add the new annotation to the list\n        this.annotations.push([lat, lng]);\n      }\n    }\n  }\n};","map":{"version":3,"names":["LMap","LTileLayer","LControl","LMarker","LPopup","components","data","zoom","center","markerCoords","annotations","methods","resetViewHandler","addAnnotationLeaflet","event","latlng","lat","lng","push"],"sources":["/Users/juangonzalez/Documents/GitHub/vue-project-kiwi/Clinet/src/components/LeafLet.vue"],"sourcesContent":["<template>\n  <div style=\"height: 85vh; width:99vw\">\n    <l-map ref=\"map\" v-model:zoom=\"zoom\" :center=\"center\" style=\"height: 100%\" @click=\"addAnnotationLeaflet\">\n      <!-- leaflet tiling -->\n      <l-tile-layer  \n        url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n        layer-type=\"base\"\n        name=\"OpenStreetMap\"\n      ></l-tile-layer>\n\n      <!-- loading in the annotations -->\n      <l-marker v-for=\"(annotation, index) in annotations\" :key=\"index\" :lat-lng=\"annotation\">  \n      </l-marker>\n\n      <!-- loading in the orthophoto tiling -->\n      <l-tile-layer\n        url=\"http://127.0.0.1:8000/cog/tiles/{z}/{x}/{y}?tileMatrixSetId=WebMercatorQuad&scale=1&url=https%3A%2F%2Fproject-kiwi-temp.s3.us-west-2.amazonaws.com%2Fodm_orthophoto.tif&unscale=false&resampling=nearest&reproject=nearest&return_mask=true\"\n        layer-type=\"base\"\n        name=\"orthophotofromTitiler\"\n      ></l-tile-layer>\n\n      <!-- reset view button \n      <l-control position=\"topleft\" >\n      <button @click=\"resetViewHandler\">\n      Reset View\n      </button>\n      </l-control> -->\n\n      <!-- individual marker\n      <l-marker :lat-lng=\"markerCoords\">\n      <LPopup>\n      Hello World\n      <label for=\"annotationName\">First name:</label>\n      <input type=\"text\" id=\"annotationName\" name=\"annotationName\"><br><br>\n      </LPopup>\n      </l-marker> -->\n        \n    </l-map>\n  </div>\n</template>\n\n<script>\nimport \"leaflet/dist/leaflet.css\";\nimport { LMap, LTileLayer, LControl, LMarker, LPopup } from \"@vue-leaflet/vue-leaflet\";\n\nexport default {\n  components: {\n    LMap,\n    LTileLayer,\n    LControl,\n    LPopup,\n    LMarker,\n},\n  data() {\n    return {\n      zoom: 17,\n      center: [47.70571705924626, -122.1419059757434],\n      markerCoords: [47.70446575151325, -122.1422571803662],\n      annotations: []\n    };\n  },\n  methods: {\n    resetViewHandler() {\n      this.center=[47.70571705924626, -122.1419059757434]\n      this.zoom=17\n    },\n\n    addAnnotationLeaflet(event) {\n    // Ensure that event has latlng property\n    if (event && event.latlng) {\n      // Extract latitude and longitude from the click event\n      const lat = event.latlng.lat;\n      const lng = event.latlng.lng;\n\n      // Add the new annotation to the list\n      this.annotations.push([lat, lng]);\n    }\n  }\n  }\n};\n\n</script>\n\n<style></style>"],"mappings":";AA0CA,OAAO,0BAA0B;AACjC,SAASA,IAAI,EAAEC,UAAU,EAAEC,QAAQ,EAAEC,OAAO,EAAEC,MAAK,QAAS,0BAA0B;AAEtF,eAAe;EACbC,UAAU,EAAE;IACVL,IAAI;IACJC,UAAU;IACVC,QAAQ;IACRE,MAAM