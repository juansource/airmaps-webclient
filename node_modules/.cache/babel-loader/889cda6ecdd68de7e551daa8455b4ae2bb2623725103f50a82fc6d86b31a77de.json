{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"leaflet/dist/leaflet.css\";\nimport { LMap, LTileLayer, LControl, LMarker, LPopup } from \"@vue-leaflet/vue-leaflet\";\nexport default {\n  components: {\n    LMap,\n    LTileLayer,\n    LControl,\n    LPopup,\n    LMarker\n  },\n  data() {\n    return {\n      zoom: 17,\n      center: [47.70571705924626, -122.1419059757434],\n      markerCoords: [47.70446575151325, -122.1422571803662],\n      annotations: []\n    };\n  },\n  methods: {\n    resetViewHandler() {\n      this.center = [47.70571705924626, -122.1419059757434];\n      this.zoom = 17;\n    },\n    addAnnotationLeaflet(event) {\n      const latitude = event.latLng.lat;\n      const longitude = event.latLng.lng;\n      this.annotations.push([latitude, longitude]);\n    }\n  }\n};","map":{"version":3,"names":["LMap","LTileLayer","LControl","LMarker","LPopup","components","data","zoom","center","markerCoords","annotations","methods","resetViewHandler","addAnnotationLeaflet","event","latitude","latLng","lat","longitude","lng","push"],"sources":["/Users/juangonzalez/Documents/GitHub/vue-project-kiwi/Clinet/src/components/LeafLet.vue"],"sourcesContent":["<template>\n  <div style=\"height: 85vh; width:99vw\">\n    <l-map ref=\"map\" @click=\"addAnnotationLeaflet\" v-model:zoom=\"zoom\" :center=\"center\" style=\"height: 100%\">\n      <!-- leaflet tiling -->\n      <l-tile-layer  \n        url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n        layer-type=\"base\"\n        name=\"OpenStreetMap\"\n      ></l-tile-layer>\n\n      <!-- loading in the annotations -->\n      <l-marker v-for=\"(annotation, index) in annotations\" :key=\"index\" :lat-lng=\"annotation\">  \n      </l-marker>\n\n      <!-- loading in the orthophoto tiling -->\n      <l-tile-layer\n        url=\"http://127.0.0.1:8000/cog/tiles/{z}/{x}/{y}?tileMatrixSetId=WebMercatorQuad&scale=1&url=https%3A%2F%2Fproject-kiwi-temp.s3.us-west-2.amazonaws.com%2Fodm_orthopho